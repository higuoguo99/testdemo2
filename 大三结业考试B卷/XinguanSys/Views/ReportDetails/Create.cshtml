@model XinguanSys.Models.ReportDetails

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    <form enctype="multipart/form-data" method="post" action="/ReportDetails/Create">
        <table>
            <tr>
                <td>地区名称：</td>
                <td> @Html.TextBoxFor(model => model.AreaName)</td>
            </tr>
            <tr>
                <td>现有确诊：</td>
                <td> @Html.TextBoxFor(model => model.CurrentNum)</td>
            </tr>
            <tr>
                <td>累计确诊：</td>
                <td> @Html.TextBoxFor(model => model.DiagnosisNum)</td>
            </tr>
            <tr>
                <td>累计治愈：</td>
                <td> @Html.TextBoxFor(model => model.CureNum)</td>
            </tr>
            <tr>
                <td>累计死亡：</td>
                <td>@Html.TextBoxFor(model => model.DeathNum)</td>
            </tr>
            <tr>
                <td>更新时间：</td>
                <td> @Html.TextBoxFor(model => model.UpdateTime)</td>
            </tr>
            <tr>
                <td>疫情图片：</td>
                <td> <input type="file" name="Image" /></td>
            </tr>
            <tr>
                <td colspan="2"> <input type="submit" value="添加" /></td>
            </tr>
        </table>
          
    
    </form>

</body>
</html>
